{
    "name": "veridock/roadrunner-svg-php",
    "description": "RoadRunner server for PHP-SVG processing with high performance",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "spiral/roadrunner": "^2023.1",
        "spiral/roadrunner-http": "^3.0",
        "nyholm/psr7": "^1.5",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "spiral/roadrunner-cli": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "rr:download": "rr get-binary",
        "rr:serve": "./rr serve",
        "rr:workers": "./rr workers -i",
        "rr:reset": "./rr reset",
        "test": "php test.php"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
